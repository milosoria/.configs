# ALIASES 
#general
alias ..='cd ..'
alias nv='nvim'
alias home='cd ~'
alias mine='cd ~/projects'
alias dotfiles='cd ~/.config'
alias ex='exit'
alias vimrc='cd ~/.config/nvim  && "$EDITOR" init.lua'
alias dotfiles='cd ~/.config'
alias kittyconf='cd ~/.config/kitty/ && "$EDITOR" kitty.conf'
alias ll='ls -lah'
alias l='ls -lah'

# git
alias ga='git add'
alias gc='git commit -m'
alias gp='git push origin'
alias gs='git status'
alias qpll='git pull origin'

# apt
alias update='brew update'
alias upgrade='brew upgrade'

# cheat sheet
alias cheat='cht.sh'

# custom commands
alias create='project'
alias qpsh='qpsh'
